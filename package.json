{
  "name": "prosoft-components-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve prosoft-components-demo",
    "build": "ng build",
    "build:components": "npm run build:components-lib && npm run build:components-schematics",
    "build:components-demo": "ng build prosoft-components-demo --configuration production --base-href https://prosoft-edv.github.io/components/",
    "build:components-lib": "ng build components --configuration production",
    "build:components-schematics": "tsc -p projects/components/tsconfig.schematics.json",
    "test": "ng test",
    "test:schematics": "npm run build:components-schematics && jasmine dist/components/schematics/**/*_spec.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "release:test": "ng lint components && ng test components --watch false",
    "release": "lerna version",
    "copy:schemas": "copyfiles -u 3 projects/components/schematics/*/schema.json dist/components/schematics/",
    "copy:files": "copyfiles -u 3 projects/components/schematics/*/files/** dist/components/schematics/",
    "copy:collection": "copyfiles -f projects/components/schematics/migrations.json dist/components/schematics/",
    "postbuild:components-schematics": "npm run copy:schemas && npm run copy:files && npm run copy:collection",
    "prepare": "husky install"
  },
  "private": false,
  "dependencies": {
    "@angular-devkit/core": "^14.0.4",
    "@angular-devkit/schematics": "^14.0.4",
    "@angular/animations": "^14.0.4",
    "@angular/cdk": "^14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/forms": "^14.0.4",
    "@angular/material": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/router": "^14.0.4",
    "ngx-mat-select-search": "^4.2.0",
    "nouislider": "^15.6.0",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.0.4",
    "@angular/compiler-cli": "^14.0.4",
    "@angular/language-service": "^14.0.4",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-rxjs": "^5.0.2",
    "husky": "^8.0.1",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jsdom": "^20.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lerna": "^5.1.6",
    "lint-staged": "^13.0.3",
    "ng-packagr": "^14.0.2",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.8.2",
    "tslint-no-focused-test": "^0.5.0",
    "typescript": "~4.7.4"
  },
  "lint-staged": {
    "*.{ts,json,scss,html}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
